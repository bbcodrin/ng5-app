{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng5-app"
  },
  "apps": [
    {
      "name": "app1",
      "root": "src",
      "outDir": "dist",
      "assets": [
        "apps/app/assets",
        "apps/app/favicon.ico"
      ],
      "index": "apps/app/index.html",
      "main": "apps/app/main.ts",
      "polyfills": "apps/app/polyfills.ts",
      "test": "apps/app/test.ts",
      "tsconfig": "apps/app/tsconfig.app.json",
      "testTsconfig": "apps/app/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "apps/app/styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "app2",
      "root": "src",
      "outDir": "dist2",
      "assets": [
        "apps/app2/assets",
        "apps/app2/favicon.ico"
      ],
      "index": "apps/app2/index.html",
      "main": "apps/app2/main.ts",
      "polyfills": "apps/app2/polyfills.ts",
      "test": "apps/app2/test.ts",
      "tsconfig": "apps/app2/tsconfig.app.json",
      "testTsconfig": "apps/app2/tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "apps/app2/styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
